{
    "header-layout.desktop": {
      "title":"Header for desktop view",
        "children": [
          "sticky-layout#header__rows-desktop"
        ]
      },
      "sticky-layout#header__rows-desktop": {
        "title":"Header elements for desktop view",
        "props": {
          "blockClass": "sticky__header-desktop"
        },
        "children": [
            "flex-layout.row#1-header__menu-item_desktop",
            "flex-layout.row#2-header__elements_desktop",
            "flex-layout.row#3-header__menu_desktop"                   
        ]
      },

   // row 1

    "flex-layout.row#1-header__menu-item_desktop" : {
        "title":"Menu items for first row-header",
        "props": {
          "horizontalAlign": "left",
          "verticalAlign": "center",
          "preventHorizontalStretch": true,
          "preventVerticalStretch": true,
          "fullWidth": true,
          "blockClass": "header-row1__menu-item_desktop"
        },
        "children": [               
          "vtex.menu@2.x:menu#sites__genero_desktop"         
        ]
      },

      "vtex.menu@2.x:menu#sites__genero_desktop":{
        "title":"Menu items in header for desktop view",
        "children": [
            "menu-item#site__leonisa_desktop",
            "menu-item#site__leo_desktop",
            "menu-item#site__aliados-leonisa_desktop",          
            "slider-layout#header__slider_desktop"              
        ]
    },

    "menu-item#site__leonisa_desktop": {
        "title":"Leonisa option as a menu item",
        "props": {
            "id": "menu-item-category-women",
            "type": "custom",
            "iconId": null,
            "highlight": false,            
            "blockClass":"item__category_leonisa-desktop",
            "itemProps": {
                "type": "internal",
                "href": "/",
                "noFollow": false,
                "tagTitle": "Women",
                "text": "LEONISA"
            }
        }
    },

