    "slider-layout#header__slider_desktop":{
      "title": "Slider-layout for header desktop",
      "props":{
        "itemsPerPage":{
          "desktop":1,
          "tablet":1,
          "phone":1
        },
        "infinite": true,
        "blockClass": "header__slider_desktop",
        "showNavigationArrows": "never", 
        "showPaginationDots": "never",            
        "autoplay":{
          "timeout":5000,
          "stopOnHover":true          
        }
      },
      "children":[
        "link#1__desktop_free-shipping",
        "link#2__desktop_returns-exchanges",
        "link#3__desktop_offers-available"                
      ]
    },
  
    "link#1__desktop_free-shipping": {
      "title":"First link of slider ",
      "props":{
        "blockClass":"link__slider-desktop_one",
        "label":"ENVIO GRATIS en órdenes de $60.000 o más ",       
        "href":"https://www.google.com/?hl=es"
      }
  
    },
    "link#2__desktop_returns-exchanges": {
      "title":"Second link of slider",
      "props":{
        "blockClass":"link__slider-desktop_two",
        "label":"CAMBIOS Y DEVOLUCIONES Sin complicaciones",
        "href":"https://www.google.com/?hl=es"        
      }
      
    },
    "link#3__desktop_offers-available": {
      "title":"Third link of slider",
      "props":{
        "blockClass":"link__slider-desktop_three",
        "label":"OFERTAS DISPONIBLES Descúbrelas",
        "href":"https://www.google.com/?hl=es"          
      }
      
    },

